        #include <iostream>

        class Example {
            public:
            void print() {
                std::cout << "Non-const print" << std::endl;
            }

            void print() const {
                std::cout << "Const print" << std::endl;
            }
        };

        void forceModify(const Example& obj) {
            const_cast<Example&>(obj).print();  // Calls non-const version
        }

        int main() {
            Example e;
            forceModify(e);  // Calls non-const print()
        }
